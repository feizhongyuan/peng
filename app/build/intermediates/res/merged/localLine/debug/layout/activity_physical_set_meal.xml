            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="@dimen/dp_10"
            android:background="@color/huise"
            >
            <ImageView
                android:id="@+id/img_taocan_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@mipmap/ic_launcher"
                android:scaleType="fitXY"
                />
            <TextView
                android:id="@+id/tv_taocan_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/name_taocanfenlei_1"
                android:layout_below="@id/img_taocan_1"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/dp_5"
                />
            <View
                android:layout_width="@dimen/dp_60"
                android:layout_height="@dimen/dp_0_1"
                android:background="@color/black"
                android:layout_below="@id/img_taocan_1"
                android:layout_toLeftOf="@id/tv_taocan_1"
                android:layout_marginTop="@dimen/dp_12"
                android:layout_marginRight="@dimen/dp_10"
                />
            <View
                android:layout_width="@dimen/dp_60"
                android:layout_height="@dimen/dp_0_1"
                android:background="@color/black"
                android:layout_below="@id/img_taocan_1"
                android:layout_marginTop="@dimen/dp_12"
                android:layout_toRightOf="@id/tv_taocan_1"
