    android:layout_height="match_parent"
    tools:context="com.maqueezu.el.ui.fragment.physicalexamination_child.SetMealListItemFragment">

    <com.scwang.smartrefresh.layout.SmartRefreshLayout
        android:id="@+id/mSmart_refresh_layout"
        android:layout_width="match_parent"
